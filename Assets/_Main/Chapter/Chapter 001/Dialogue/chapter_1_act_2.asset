      characterData: {fileID: 11400000, guid: e520f1218f361014b95df81d90e00edd, type: 2}
      inChargeState: 0
      expression: 4
    chapterDialogueEventList: []
  - text: (Among the group, a woman in a green dress stays silent, her eyes averted,
      seeming to be deep in thought)
