      expression: 0
    - name: Rosalind Capet | Show
      characterData: {fileID: 11400000, guid: e520f1218f361014b95df81d90e00edd, type: 2}
      inChargeState: 0
      expression: 0
    chapterDialogueEventList: []
  - text: Ah yes, I've heard of you, Sir Wakefield. I'm Henry Fairfax, nice to meet
      you.
    characterData: {fileID: 11400000, guid: c7056aca07669fb4180a757ca52d9c2d, type: 2}
    characterInChargeList:
    - name: Elias Wakefield | Show
      characterData: {fileID: 11400000, guid: c7056aca07669fb4180a757ca52d9c2d, type: 2}
