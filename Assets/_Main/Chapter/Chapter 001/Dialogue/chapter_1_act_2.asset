      inChargeState: 0
      expression: 0
    chapterDialogueEventList: []
  - text: I've been overworking myself. Excuse me, I need a drink.
    characterData: {fileID: 11400000, guid: e3e3b01075cfef5448786d6b861fb316, type: 2}
