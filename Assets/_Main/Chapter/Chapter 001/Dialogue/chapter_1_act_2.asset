      expression: 0
    - name: Reginal Blackwood | Show
      characterData: {fileID: 11400000, guid: c9e6580d3476f524d805f8c80a42de99, type: 2}
      inChargeState: 0
      expression: 2
    chapterDialogueEventList: []
  - text: No worries, my lord. It's fine.
    characterData: {fileID: 11400000, guid: c7056aca07669fb4180a757ca52d9c2d, type: 2}
    characterInChargeList:
    - name: Elias Wakefield | Show
