      audioClip: {fileID: 0}
      prefab: {fileID: 0}
  - text: (Elias steps inside, greeted by an older man in a suit)
    characterData: {fileID: 0}
    characterInChargeList:
    - name: Elias Wakefield | Show
      characterData: {fileID: 11400000, guid: c7056aca07669fb4180a757ca52d9c2d, type: 2}
      inChargeState: 0
      expression: 0
    - name: Security Guard | Show
      characterData: {fileID: 11400000, guid: a8241fb1ead44e047a1486d07917a732, type: 2}
      inChargeState: 0
      expression: 0
    chapterDialogueEventList: []
  - text: Good evening, Sir Elias. I am Alistair Henshaw, let me lead you to the
      ballroom while our maid Edith here brings your luggage to your room.
    characterData: {fileID: 11400000, guid: 0492e72e6b9e2f74296421603bf523c7, type: 2}
    characterInChargeList:
    - name: Elias Wakefield | Show
      characterData: {fileID: 11400000, guid: c7056aca07669fb4180a757ca52d9c2d, type: 2}
      inChargeState: 0
      expression: 0
    - name: Security Guard | Hide
      characterData: {fileID: 11400000, guid: a8241fb1ead44e047a1486d07917a732, type: 2}
      inChargeState: 1
      expression: 0
    - name: Alistair Henshaw | Show
      characterData: {fileID: 11400000, guid: 0492e72e6b9e2f74296421603bf523c7, type: 2}
      inChargeState: 0
      expression: 0
    chapterDialogueEventList: []
  - text: Thank you.
    characterData: {fileID: 11400000, guid: c7056aca07669fb4180a757ca52d9c2d, type: 2}
    characterInChargeList:
    - name: Elias Wakefield | Show
      characterData: {fileID: 11400000, guid: c7056aca07669fb4180a757ca52d9c2d, type: 2}
      inChargeState: 0
      expression: 0
